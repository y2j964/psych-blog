{
  "env": {
    "es6": true,
    "browser": true
  },
  "settings": {},
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "hbs"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {},
    "rules": {
      "hbs/check-hbs-template-literals": 2,
      "indent": [
        "error",
        2,
        {
          "ignoreComments": true
        }
      ],
      "prettier/prettier": "error"
    }
  }
}
